<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#9933b5e5" />
            <stroke android:width="1dp" android:color="#99cc" />
            <corners android:radius="3dp" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="#6633b5e5" />
            <stroke android:width="1dp" android:color="#ddff" android:dashWidth="3dp" android:dashGap="3dp" />
            <corners android:radius="3dp" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#0000" />
        </shape>
    </item>
</selector>
